{
  "name": "@gilbarbara/types",
  "version": "0.1.1",
  "description": "Reusable typescript typings",
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "keywords": [
    "typescript",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gilbarbara/types"
  },
  "bugs": {
    "url": "https://github.com/gilbarbara/types/issues"
  },
  "homepage": "https://github.com/gilbarbara/types",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "types": "./lib",
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@gilbarbara/eslint-config": "^0.5.1",
    "@gilbarbara/prettier-config": "^1.0.0",
    "@gilbarbara/tsconfig": "^0.1.1",
    "del-cli": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "repo-tools": "^0.2.2",
    "ts-node": "^10.9.1",
    "type-fest": "^4.1.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "build": "npm run clear && npm run build:lib",
    "build:lib": "tsc",
    "clear": "del lib/*",
    "lint": "eslint --ext .ts,.tsx src",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run build",
    "prepare": "husky install",
    "prepublishOnly": "npm run validate"
  },
  "eslintConfig": {
    "extends": [
      "@gilbarbara/eslint-config"
    ]
  },
  "prettier": "@gilbarbara/prettier-config"
}
